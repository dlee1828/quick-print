{
  "name": "quick-print",
  "displayName": "quick-print",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-print.quickPrintVariable",
        "title": "Quick Print Variable"
      },
      {
        "command": "quick-print.quickPrintLine",
        "title": "Quick Print Line"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "quickPrint.printVariableTemplates": {
          "type": "object",
          "properties": {
            "cpp": {
              "type": "string",
              "default": "std::cout << \"QP_NAME: \" << QP_VALUE << \"\\n\";\n"
            }
          }
        },
        "quickPrint.printLineTemplates": {
          "type": "object",
          "properties": {
            "cpp": {
              "type": "string",
              "default": "std::cout << \"QP_LINE\" << \"\\n\";\n"
            }
          }
        }
      }
    },
    "keybindings": [
      {
        "command": "quick-print.quickPrintLine",
        "key": "alt+cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "quick-print.quickPrintVariable",
        "key": "alt+cmd+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}